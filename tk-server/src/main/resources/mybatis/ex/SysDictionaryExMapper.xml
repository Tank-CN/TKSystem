<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tk.mapper.ex.SysDictionaryExMapper">
	<resultMap id="BaseResultMap" type="com.tk.model.SysDictionary" extends="com.tk.mapper.SysDictionaryMapper.BaseResultMap">
	</resultMap>
	<cache-ref namespace="com.tk.mapper.SysDictionaryMapper" />
	<select id="selectMaxIID" parameterType="java.lang.Integer" resultType="java.lang.Integer">
		SElECT max(a.IID) IID from
		(select * from sys_dictionary where PID = #{parentId,jdbcType=INTEGER} or id =
		#{parentId,jdbcType=INTEGER}) as a
	</select>

	<select id="selectMaxCID" resultType="java.lang.Integer">
		SELECT MAX(CID) FROM sys_dictionary
	</select>

	<insert id="insertAndGetId" parameterType="com.tk.model.SysDictionary" keyProperty="iid">
		insert into sys_dictionary
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="iid != null">
				IID,
			</if>
			<if test="cid != null">
				CID,
			</if>
			<if test="pid != null">
				PID,
			</if>
			<if test="sn != null">
				SN,
			</if>
			<if test="title != null">
				TITLE,
			</if>
			<if test="alias != null">
				ALIAS,
			</if>
			<if test="idx != null">
				IDX,
			</if>
			<if test="sort != null">
				SORT,
			</if>
			<if test="gbcode != null">
				GBCODE,
			</if>
			<if test="isocode != null">
				ISOCODE,
			</if>
			<if test="flag != null">
				FLAG,
			</if>
			<if test="memo != null">
				MEMO,
			</if>
			<if test="source != null">
				SOURCE,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="iid != null">
				#{iid,jdbcType=BIGINT},
			</if>
			<if test="cid != null">
				#{cid,jdbcType=VARCHAR},
			</if>
			<if test="pid != null">
				#{pid,jdbcType=BIGINT},
			</if>
			<if test="sn != null">
				#{sn,jdbcType=VARCHAR},
			</if>
			<if test="title != null">
				#{title,jdbcType=VARCHAR},
			</if>
			<if test="alias != null">
				#{alias,jdbcType=VARCHAR},
			</if>
			<if test="idx != null">
				#{idx,jdbcType=VARCHAR},
			</if>
			<if test="sort != null">
				#{sort,jdbcType=SMALLINT},
			</if>
			<if test="gbcode != null">
				#{gbcode,jdbcType=VARCHAR},
			</if>
			<if test="isocode != null">
				#{isocode,jdbcType=VARCHAR},
			</if>
			<if test="flag != null">
				#{flag,jdbcType=TINYINT},
			</if>
			<if test="memo != null">
				#{memo,jdbcType=VARCHAR},
			</if>
			<if test="source != null">
				#{source,jdbcType=VARCHAR},
			</if>
		</trim>
	</insert>
</mapper>